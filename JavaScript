function doPost(e) {
  // Obtém os dados enviados pelo formulário
  var params = JSON.parse(e.postData.contents);
  
  // Aqui você pode processar os dados e salvar em uma planilha, por exemplo
  var sheet = SpreadsheetApp.openById('162P2kwnI9oANMrFV64kTtNGpEi7fdaRdqN-IPHK6N2Q').getActiveSheet();
  sheet.appendRow([params.nome, params.serie, params.data, params.resposta1, params.resposta2, params.tempos]);

  // Retorna uma resposta para o cliente
  return ContentService.createTextOutput(JSON.stringify({
    status: 'success',
    feedback: ['correto', 'incorreto', 'correto', 'correto', 'incorreto', 'correto', 'incorreto', 'correto', 'correto', 'correto']
  })).setMimeType(ContentService.MimeType.JSON);
}



